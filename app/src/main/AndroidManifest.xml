<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="spidey.com.reyaj.chat.sams">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_action_done_all"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_action_done_all"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".profile.ProfileActivity"
            android:parentActivityName=".AppBase" />
        <activity
            android:name=".profile.StudentRegistration"
            android:parentActivityName=".profile.ProfileActivity" />
        <activity
            android:name=".attendance.AttendanceActivity"
            android:parentActivityName=".AppBase" />
        <activity
            android:name=".profile.EditStudentActivity"
            android:parentActivityName=".AppBase" />
        <activity
            android:name=".components.SettingsActivity"
            android:parentActivityName=".AppBase" />
        <activity
            android:name=".components.About"
            android:parentActivityName=".AppBase" />
        <activity android:name=".AppBase" />
        <activity android:name=".StudActivity"></activity>
    </application>

</manifest>